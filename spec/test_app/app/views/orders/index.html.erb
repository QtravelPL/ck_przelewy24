<h1> Create Test Order </h1>
<form accept-charset="UTF-8" action="/orders" method="post">
  <input name="utf8" type="hidden" value="&#x2713;" />
  <label for="first_name">First Name:</label>
  <input id="first_name" name="first_name" type="text" required="true" />

  <label for="last_name">Last Name:</label>
  <input id="last_name" name="last_name" type="text" required="true" />

  <label for="email">Email:</label>
  <input id="email" name="email" type="text" required="true" />

  <label for="phone">Phone:</label>
  <input id="phone" name="phone" type="text" required="true" />

  <label for="price">Price:</label>
  <input id="price" name="price" type="text" required="true" />

  <input name="commit" type="submit" value="Create Order and go to payment" />
</form>